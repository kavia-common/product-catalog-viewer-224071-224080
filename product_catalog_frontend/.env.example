# Frontend environment variables (no secrets)
REACT_APP_API_BASE=http://localhost:8080/api
REACT_APP_BACKEND_URL=http://localhost:8080
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_WS_URL=ws://localhost:8080/ws
REACT_APP_NODE_ENV=development
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/health
# Comma-separated feature flags. Add "mock_api" to use built-in mock data when backend is unavailable.
REACT_APP_FEATURE_FLAGS=mock_api
REACT_APP_EXPERIMENTS_ENABLED=false
